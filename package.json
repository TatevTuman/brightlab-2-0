    "build": "dotenv gatsby build",
    "develop": "dotenv gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "dotenv npm run develop",
    "serve": "dotenv gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "server": "nodemon server/index.js",
    "db:setup": "env NODE_ENV=development /bin/bash static/scripts/setup.sh",
    "type-check": "tsc --noEmit",
    "types-generate": "graphql-codegen --config codegen.yml"
  },
  "nodemonConfig": {
    "ignore": ["static/*", "src/*", "public/*", "node_modules/*", "apollo/*", "__tests__/*", "__mocks__/*", ".jest/*", ".cache/*"],
